{
    "builds": [
        {
            "src": "api/flask_server_gemini_ukai.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/flask_server_gemini_ukai.py"
        }
    ]
}